{
  "name": "tipi",
  "displayName": "tipi",
  "description": "tipi-lang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "tipi",
      "aliases": ["tipi-lang", "tipi"],
      "extensions": [".tipi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tipi",
      "scopeName": "source.tipi",
      "path": "./syntaxes/tipi.tmLanguage.json"
    }],
    "semanticTokenScopes": [
      {
        "scopes": {
          "tipi.custom.scope": ["annotation.tipi"]
        }
      }
    ]
  }
}
